multibranchPipelineJob('springbootk8sjenkinsopexample') {
    branchSources {
        git {
            id('springbootk8sjenkinsopexample') // IMPORTANT: use a constant and unique identifier
            url('https://github.com/bbdsoftware/eks-pring-boot-jenkinsop-example.git')
            credentials('jenkins-github')
            includes('*')
        }
    }
    orphanedItemStrategy {
        discardOldItems {
        }
    }

    triggers {
        periodic(1)

    }

}